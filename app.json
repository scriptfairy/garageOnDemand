{
  "expo": {
    "name": "GarageOnDemand",
    "slug": "GarageOnDemand",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./app/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alfairy.GarageOnDemand"
    },
    "description": "List/Hire a garage",
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.alfairy.GarageOnDemand"
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    }
  }
}
